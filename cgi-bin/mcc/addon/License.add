#Template.pl
$License_version="1.09.0316";
$license{'1'} = "License information";
$license{'2'} = "This version of Member Control Centre is,<BR>licensed to: $licensename<BR>for website:";
$license{'3'} = "License information";
$license{'4'} = "Licensed to";
$license{'5'} = "Website";
$license{'6'} = "License code";
$license{'7'} = "Copyright footer";

License_validate();
#sub License_AdminMenu{}
sub License_AdminPanel{
	if (!$validlicense) {return;}
		$subsmain .= qq~<table border="0" cellpadding="5" cellspacing="1" align="center" bgcolor="$color{'bordercolor'}" class="bordercolor" width="100%">
			<tr>
			<td class="catbg" bgcolor="$color{'catbg'}" colspan="2"><img src="$imagesdir/info.gif" alt="" border="0"> <font size="2"><B>$license{'1'}</B></font></td>
			</tr><tr>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}" colspan="2">
				<font size="1">$license{'2'} $licensesitename</font>
				</font></td>
			</tr>
		</table><BR>~;
}

#Add items to the settings page, Create row and add it to $subsmain
sub License_SettingsPage{
		$subsmain .= qq~<tr>
				<td class="titlebg" bgcolor="$color{'titlebg'}" colspan=2>
					<font size=2 class="text1" color="$color{'titletext'}"><b>$license{'3'}</b></font></td>
			</tr><tr>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><font size="2">$license{'4'}</font></td>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><input type=text name="licensename" size="40" value="$licensename"></td>
		</tr><tr>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><font size="2">$license{'5'}</font></td>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><input type=text name="licensesite" size="40" value="$licensesite"></td>
		</tr><tr>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><font size="2">$license{'6'}</font></td>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><input type=text size="40" name="licensecode" value="$licensecode"></td>
		</tr><tr>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><font size="2">$license{'7'}</font></td>
				<td class="windowbg2" bgcolor="$color{'windowbg2'}"><textarea rows=4 cols=30 name="licensefooter">$licensefooter</textarea></td>
		</tr>~;
}

#Save items of settings page. Add settings stuff in $addonsettings
sub License_SettingsSave{
	$licensename = $FORM{'licensename'};
	$licensesite = $FORM{'licensesite'};
	$licensecode = $FORM{'licensecode'};
	$licensefooter = $FORM{'licensefooter'};
	$addonsettings .= qq~########## License Settings ###########
\$licensename = "$licensename"; #License given to name
\$licensesite = "$licensesite"; #License given to website
\$licensecode = q^$licensecode^; #License code used
\$licensefooter = q^$licensefooter^; #License footer replaces copyright
~;
}

sub License_validate {
  $licensesitename = $licensesite;
	$l234055021045040130303="";$l04535501040336="";$l340630630630625334636="";$l02353603479472046364="";ll($l04535501040336);$l02353690335253446327="";
}
1;

