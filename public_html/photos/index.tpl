<!-- BEGIN CONTENT -->
<h1>{TITLE}</h1>

<div class="card-page">
  <!-- BEGIN CARD -->
  <div class="card {STYLE} mb-2">
    <a href="{URL}">
      <div class="card-body" style="overflow: auto">
        <!-- BEGIN THUMBNAIL -->
        <img src="{THUMBNAIL-URL}" class="float-right ml-2"/>
        <!-- END THUMBNAIL -->
        <div class="float-both">
        <h3 class="card-title">{TITLE}</h3>
        <p class="card-text">{TEXT}</p>
        </div>
      </div>
    </a>
  </div>
  <!-- END CARD -->
</div>

<!-- END CONTENT -->
